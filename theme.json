			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#b267e6"
			}
		},
		{
			"name": "Modules",
			"scope": "support.module, support.node",
			"settings": {
				"foreground": "#9fcff9",
				"fontStyle": ""
			}
		},
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"foreground": "#56658b",
				"fontStyle": ""
			}
		},
		{
			"name": "Class",
			"scope": "entity.name.type.class",
			"settings": {
				"foreground": "#f7e1a5",
				"fontStyle": ""
			}
		},
		{
			"name": "C# class",
			"scope": "source.cs meta.class.identifier storage.type",
			"settings": {
				"foreground": "#21C5C7",
				"fontStyle": "bold"
			}
		},
		{
			"name": "C# type",
			"scope": "source.cs storage.type",
			"settings": {
				"foreground": "#aed0ec"
			}
